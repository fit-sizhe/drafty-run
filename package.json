{
  "name": "mdrun",
  "displayName": "Markdown Code Runner",
  "description": "Run code blocks in markdown files with state persistence",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mdrun.startSession",
        "title": "mdrun: Start Markdown Code Session"
      },
      {
        "command": "mdrun.runNextBlock",
        "title": "mdrun: Run Next Code Block"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "vsce package",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/markdown-it": "^14.1.2",
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.18.63",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.96.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "glob": "^7.2.3",
    "mocha": "^8.4.0",
    "typescript": "^4.9.5",
    "vsce": "^2.15.0",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "markdown-it": "^12.3.2",
    "python-shell": "^5.0.0",
    "uuid": "^9.0.1"
  }
}
