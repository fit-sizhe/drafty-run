{
  "name": "mdrun",
  "displayName": "Markdown Code Runner",
  "description": "Run code blocks in markdown files with state persistence",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mdrun.startSession",
        "title": "Start Markdown Code Session"
      },
      {
        "command": "mdrun.runNextBlock",
        "title": "Run Next Code Block"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "vsce package",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/markdown-it": "^14.1.2",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.18.63",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "markdown-it": "^12.0.0",
    "python-shell": "3.0.1"
  }
}
